   "source": [
    "employees = [\n",
    "    {\"name\": \"John\", \"salary\": 45000},\n",
    "    {\"name\": \"Mary\", \"salary\": 52000},\n",
    "    {\"name\": \"Alex\", \"salary\": 48000}\n",
    "]\n",
    "\n",
    "# Sort by salary\n",
    "sorted_employees = sorted(employees, key=lambda x: x[\"salary\"])\n",
    "\n",
    "# Display\n",
    "print(\"Sorted by Salary:\")\n",
    "print(sorted_employees)\n"
   ]
  }
