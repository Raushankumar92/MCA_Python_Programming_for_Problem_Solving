   "source": [
    "# reverse a number.\n",
    "num = int(input(\"Enter a number: \"))\n",
    "\n",
    "rev = 0\n",
    "temp = num\n",
    "\n",
    "while temp > 0:\n",
    "    digit = temp % 10\n",
    "    rev = rev * 10 + digit\n",
    "    temp //= 10\n",
    "\n",
    "print(\"Reversed number:\", rev)\n"
   ]
  }
